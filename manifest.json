{
  "name": "YT Playlist Search",
  "version": "1.0",
  "manifest_version": 3,
  "description": "adds a search input, to search inside a given playlist",
  "permissions": ["scripting", "tabs", "webNavigation"],
  "host_permissions": ["https://*.youtube.com/*"],
  "background": {
    "service_worker": "background.js"
  },
   "icons": {
    "32": "icon32.png",
    "64": "icon64.png"
  }
}